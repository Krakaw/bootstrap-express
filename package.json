{
  "name": "contractinator",
  "version": "0.0.1",
  "description": "Fetch NFT data",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r tsconfig-paths/register -r ts-node/register  \"./src/test/**/*.ts\" ",
    "dev": "nodemon",
    "start": "node -r ./bootstrap.js build/app.js",
    "build": "tsc --build tsconfig.build.json",
    "lint": "eslint \"./src/**/*.ts\"",
    "prettier:check": "prettier --config .prettierrc \"./src/**/*.+(js|json|ts)\" --check --ignore-unknown",
    "prettier:write": "prettier --config .prettierrc \"./src/**/*.+(js|json|ts)\" --write --ignore-unknown"
  },
  "lint-staged": {
    "./src/**/*.+(ts|js)": [
      "npm run lint",
      "npm run prettier:check"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Krakaw/contractinator.git"
  },
  "author": "Krakaw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Krakaw/contractinator/issues"
  },
  "homepage": "https://github.com/Krakaw/contractinator#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
